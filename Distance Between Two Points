#include <stdio.h>
#include<math.h>
struct point 
{
  int x;
  int y;
};
void dist(struct point s1,struct point s2)
{
    double d=0;
    int X,Y;
    X=s2.x-s1.x;
    Y=s2.y-s1.y;
    d=pow(X,2)+pow(Y,2);
    printf("The distance between the given coordinates: %.2lf\n",sqrt(d));
}
int main()
{
    struct point S1;
    struct point S2;
    printf("Enter coordinates of point 1:\n");
    printf("Enter x coordinate of point 1:");
    scanf("%d",&S1.x);
    printf("Enter y coordinate of point 1:");
    scanf("%d",&S1.y);
    printf("Enter x coordinate of point 2:");
    scanf("%d",&S2.x);
    printf("Enter y coordinate of point 2:");
    scanf("%d",&S2.y);
    dist(S1,S2);
    
    
    return 0;
}
