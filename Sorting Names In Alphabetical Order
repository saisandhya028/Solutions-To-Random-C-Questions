#include<stdio.h>
#include<string.h>
int main()
{
    int n,i,j;
    printf("Enter the number of students in a class:");
    scanf("%d",&n);
    getchar();
    char arr[n][50];
    printf("Enter the Names one by one:");
    for(i=0;i<n;i++)
    {
        fgets(arr[i],sizeof(arr[i]),stdin);
        int j=0;
        while(arr[i][j]!='\0')
        {
            if(arr[i][j]=='\n')
            {
                arr[i][j]='\0';
                break;
            }
            j++;
        }
    }
    char swap[100];
    FILE *fp=fopen("sorted.txt","w");
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(strcmp(arr[i],arr[j])>0)
            {
            strcpy(swap,arr[i]);
            strcpy(arr[i],arr[j]);
            strcpy(arr[j],swap);  
            }
        }
    }
    for(i=0;i<n;i++)
    {
        fprintf(fp,"%s\n",arr[i]);
    }
    char names[50];
    fclose(fp);
    FILE *fp1=fopen("sorted.txt","r");
    while(fgets(names,sizeof(names),fp1)!=NULL)
    {
        printf("%s",names);
    }
    fclose(fp1);
    return 0;
}
