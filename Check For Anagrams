#include <stdio.h>
#include<string.h>

int main()
{
   char str1[100],str2[100];
   printf("Enter string 1:");
   fgets(str1,sizeof(str1),stdin);
   printf("Enter string2:");
   fgets(str2,sizeof(str2),stdin);
   int len1,len2,i,count=0,j;
   len1=strlen(str1);
   len2=strlen(str2);
   if(len1==len2)
   {
       for(i=0;i<len1;i++)
       {
           for(j=0;j<len1;j++)
           {
               if(str1[i]==str2[j])
               {
                   count++;
               }
           }
       }
   if(count==len1)
   {
       printf("Anagrams\n");
   }
   else
   {
       printf("They aren't anagrams\n");
   }
   }
   else
   {
       printf("Anagrams have same length, hence these aren't anagrams\n");
   }
    return 0;
}
