#include <stdio.h>
void mul(int A[][10],int B[][10],int C[][10],int  r1, int c1, int c2)
{
    int i,j,k;
    for(i=0;i<r1;i++)
    {
        for(j=0;j<c2;j++)
        {
            C[i][j]=0;
            for(k=0;k<c1;k++)
            {
                C[i][j]+=A[i][k]*B[k][j];
            }
        }
    }
}
void display(int C[][10],int rows,int columns)
{
    int i,j;
    for(i=0;i<rows;i++)
    {
        for(j=0;j<columns;j++)
        {
            printf("%d ",C[i][j]);
        }
        printf("\n");
    }
}
int main()
{
    int m,n,p,q;
    printf("Enter no.of rows in mat A:");
    scanf("%d",&m);
    printf("Enter no.of columns in mat A:");
    scanf("%d",&n);
    printf("Enter no.of rows in mat B:");
    scanf("%d",&p);
    printf("Enter no.of columns in mat B:");
    scanf("%d",&q);
    while(n!=p)
    {
        printf("Multiplication criteria not met, enter again\n");
        printf("Enter no.of rows in mat A:");
        scanf("%d",&m);
        printf("Enter no.of columns in mat A:");
        scanf("%d",&n);
        printf("Enter no.of rows in mat B:");
        scanf("%d",&p);
        printf("Enter no.of columns in mat B:");
        scanf("%d",&q);
    }
    int A[10][10],B[10][10],C[10][10];
    int i,j;
    printf("Enter elements of mat A:\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&A[i][j]);
        }
        
    }
    printf("Enter elements of mat B:\n");
    for(i=0;i<p;i++)
    {
        for(j=0;j<q;j++)
        {
            scanf("%d",&B[i][j]);
        }
        
    }
    mul(A,B,C,m,n,q);
    printf("Result:\n");
    display(C,m,q);
    
    return 0;
}
